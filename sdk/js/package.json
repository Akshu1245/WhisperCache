{
  "name": "@whispercache/sdk",
  "version": "0.1.0",
  "description": "WhisperCache SDK - Privacy-preserving AI memory as a service",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "whispercache",
    "privacy",
    "ai",
    "memory",
    "zk-proofs",
    "midnight",
    "cardano",
    "blockchain"
  ],
  "author": "WhisperCache Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/whispercache/whispercache.git",
    "directory": "sdk/js"
  },
  "bugs": {
    "url": "https://github.com/whispercache/whispercache/issues"
  },
  "homepage": "https://whispercache.io",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {},
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  }
}
